include "./includes.nitro"

let new_compiler: Boolean = true

fun main() {
    println("New compiler: $new_compiler")
    let args = Runtime::get_program_args()
    let output = args[0].get_or_default("output.wasm")
    println("Output path: $output")

    CompilationUnit::compile_all("../nitro/compiler/example.nitro", output)
}
